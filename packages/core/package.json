{
    "name": "@zephracss/core",
    "version": "0.1.1",
    "description": "An elegant atomic css engine for a more civilized age.",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./dist/index.js": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./dist/index.cjs": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "author": "TheCommieAxolotl",
    "keywords": [
        "zephra",
        "zephracss",
        "atomic-css",
        "css",
        "cli",
        "tailwind",
        "tailwindcss",
        "windicss"
    ],
    "license": "MIT",
    "homepage": "https://github.com/zephracss/zephracss#readme",
    "bugs": {
        "url": "https://github.com/zephracss/zephracss/issues"
    },
    "funding": {
        "type": "github",
        "url": "https://github.com/zephracss/zephracss?sponsor=1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/zephracss/zephracss.git"
    },
    "types": "index.d.ts",
    "files": [
        "dist",
        "index.d.ts"
    ],
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.0",
        "@rollup/plugin-node-resolve": "^13.0.6",
        "@rollup/plugin-terser": "^0.4.3",
        "@rollup/plugin-typescript": "^8.2.1",
        "@zephracss/common": "workspace:*",
        "jiti": "^1.18.2",
        "rollup": "^2.56.3",
        "rollup-plugin-tsconfig-paths": "^1.5.0",
        "tslib": "^2.3.0"
    },
    "dependencies": {
        "typescript": ">=4.7.2 <5.0.0 || >=5.0.0 <6.0.0"
    }
}
